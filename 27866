#include<stdio.h>
int main()
{
    char arr[1000];
    int i = 0;;
    for (; i < 1000; i++) {
        scanf("%c", &arr[i]);
        if (arr[i] == '\n')break;
    }
    for (; i < 1000; i++) {
        arr[i] = '\0';
    }

    int n;
    scanf("%d", &n);
    
    printf("%c", arr[n-1]);

    return 0;
}
