#include<stdio.h>
int main()
{
    int n;
    scanf("%d", &n);
    
    int arr[n];
    
    for(int i=0;i<n;i++){
        scanf("%d", &arr[i]);
    }
    int v;
    scanf("%d", &v);
    
    
    int count=0;
    for(int i=0;i<n;i++){
        if(v==arr[i])count++;
    }
    
    printf("%d", count);
    
    return 0;
}
