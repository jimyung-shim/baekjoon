#include<stdio.h>
#define MAX 100000
int stack[MAX];
int top = -1;

void push(int x) {
	if (top < MAX - 1) {
		stack[++top] = x;
	}
}

void pop() {
	if (top > -1) {
		top--;
	}
}

int main()
{
	int k;
	scanf_s("%d", &k);

	for (int i=0; i < k; i++) {
		int a;
		scanf_s("%d", &a);

		if (a == 0) {
			pop();
		}

		else push(a);
	}

	int sum = 0;
	for (int j = 0; j <= top; j++) {
		sum += stack[j];
	}

	printf("%d", sum);
	return 0;
}
