// 10811번

#include<stdio.h>
int main()
{
	int n, m;
	scanf_s("%d %d", &n, &m);

	int arr[100]={0};
	for (int i = 0; i < n; i++) {
		arr[i] = i + 1;
	}

	for (int i = 0; i < m; i++) {
		int a, b;
		scanf_s("%d %d", &a, &b);

		a--;
		b--;

		
		while (a < b) {
			int temp = arr[a];
			arr[a] = arr[b];
			arr[b] = temp;

			a++;
			b--;
		}
	}

	for (int k = 0; k < n; k++) {
		printf("%d ", arr[k]);
	}
	return 0;
}

// 11659번

#include<stdio.h>
int main()
{
	int n, m;
	scanf_s("%d %d", &n, &m);

	int arr[100000];

	for (int i = 0; i < n; i++) {
		arr[i] = n - i;
	}

	for (int i = 0; i < n; i++)printf("%d ", arr[i]);
	printf("\n");

	for (int i = 0; i < m; i++) {
		int a, b;
		scanf_s("%d %d", &a, &b);

		a--;
		b--;

		int sum = 0;
		for (int j = a; j <= b; j++) {
			sum += arr[j];
		}

		printf("%d\n", sum);
	}


	return 0;
}
