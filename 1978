#include<stdio.h>
int main()
{
    int n;
    scanf("%d", &n);
    int a=0;
    for(int i=0;i<n;i++){
        int m;
        int count=0;
        scanf("%d", &m);
        for(int j=1;j<=m;j++){
            if(m%j==0)count++;
        }
        if(count == 2)a++;
    }
    printf("%d", a);
    return 0;
}
